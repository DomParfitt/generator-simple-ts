{
  "name": "generator-simple-ts",
  "version": "1.0.0",
  "description": "A Yeoman Generator for a simple Typescript project structure",
  "main": "generators/app/index.js",
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "author": "Dom Parfitt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DomParfitt/generator-simple-ts.git"
  },
  "dependencies": {
    "generator-license": "^5.4.0",
    "yeoman-generator": "^4.13.0"
  },
  "devDependencies": {
    "jest": "^29.0.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-environment": "^3.4.1",
    "yeoman-test": "^6.1.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/generators/app"
    ],
    "modulePathIgnorePatterns": [
      "templates/*"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageDirectory": "test-results/jest/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**"
    ],
    "reporters": [
      "default"
    ]
  }
}
